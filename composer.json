{
    "name": "aegonplatform/gateway-pub-sample",
    "description": "Gateway Pub Sample",
    "type": "package",
    "keywords": [
        "gateway",
        "pub",
        "sample"
    ],
    "homepage": "https://github.com/aegonplatform/gateway-pub-sample",
    "license": "MIT",
    "require": {
        "php": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "GatewayPub\\": "php/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GatewayPub\\Test\\": "php/tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "example": "php -f php/examples/index.php",
        "webhook": "php -sS localhost:8080 php/examples/webhook.php"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    }
}
